    <script>
        $("tr").dblclick(function () {
            $("#table1").hide();
            $('#table1').parents('div.dataTables_wrapper').first().hide();
            var j = this.children[0].innerText;
            $.get("/dashboard/billing/" + j, function (data, status) {
            }).done(function (data) {
                console.log(data);
                $("#info-box input[name=Name]").val(data.Name);
                $("#info-box input[name=Username]").val(data.Username);
                $("#info-box input[name=Room]").val(data.Room);
                $("#info-box input[name=DateIn]").val(data.DateIn);
                $("#info-box input[name=DateOut]").val(data.DateOut);
                $("#info-box input[name=Price]").val(data.Price);
                $("#info-box input[name=Fee]").val(data.Fee);
                $("#info-box input[name=Others]").val(data.Others);
                $("#info-box input[name=Discount]").val(data.Discount);
                $("#info-box input[name=Total]").val(data.Total);
                $("#info-box").show();
            }
            );
        }); 
    </script>
    <script>
